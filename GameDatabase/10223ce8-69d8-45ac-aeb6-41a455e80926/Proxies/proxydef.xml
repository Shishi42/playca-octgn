			<match name="Type" value="License" />
		</matches>
		<overlayblocks>
			<link block="LicenseBack" />
			<conditional>
				<switch property="Team">
					<case contains="Eisei Gakuen"><link block="LicenseTEi" /></case>
					<case contains="Hakuren"><link block="LicenseTHaku" /></case>
					<case contains="Kidokawa Seishuu"><link block="LicenseTKido" /></case>
					<case contains="Mikage Sennou"><link block="LicenseTMika" /></case>
					<case contains="Minodouzan"><link block="LicenseTMino" /></case>
					<case contains="Old Raimon"><link block="LicenseTORai" /></case>
					<case contains="Outei Tsukinomiya"><link block="LicenseTOutei" /></case>
					<case contains="Raimon"><link block="LicenseTRai" /></case>
					<case contains="Seishou Gakuen"><link block="LicenseTSei" /></case>
					<case contains="Teikoku Gakuen"><link block="LicenseTTei" /></case>
					<case contains="Tonegawa Tousen"><link block="LicenseTTone" /></case>
					<case contains="Zeus"><link block="LicenseTZeus" /></case>
					<case contains="Inazuma Japan"><link block="LicenseTIna" /></case>
					<case contains="Red Bison"><link block="LicenseTRed" /></case>
					<case contains="Shining Satans"><link block="LicenseTShi" /></case>
					<case contains="Eternal Dancers"><link block="LicenseTEter" /></case>
					<case contains="Phoenix Army of Arab"><link block="LicenseTPho" /></case>
					<case contains="Soccer Acrobatic Troupe"><link block="LicenseTSoc" /></case>		
				</switch>
			</conditional>
			<link block="LChar" />
			<conditional>
				<switch property="SubType">
					<case value="Captain"><link block="LCAPTAIN" /></case>
					<case value="Captain Commitee"><link block="LCAPTAINCOMMITEE" /></case>
					<case value="Commitee"><link block="LCOMMITEE" /></case>
					<case value="CoroCoro"><link block="LCORO" /></case>
					<case value="Hissatsu"><link block="LHISSATSU" /></case>
					<case value="Hissatsu Captain"><link block="LHISSATSUCAPTAIN" /></case>
					<case value="Hissatsu Commitee"><link block="LHISSATSUCOMMITEE" /></case>
					<case value="PlayCa"><link block="LPLAYCA" /></case>
					<case value="PlayCa Captain"><link block="LPLAYCACAPTAIN" /></case>
					<case value="Promo"><link block="LPROMO" /></case>
					<case value="Promo Captain"><link block="LPROMOCAPTAIN" /></case>
					<case value="Promo Hissatsu"><link block="LPROMOHISSATSU" /></case>					
				</switch>
			</conditional>
			<conditional>
				<switch property="Position">
					<case value="DF"><link block="LDF" /></case>
					<case value="MF"><link block="LMF" /></case>
					<case value="FW"><link block="LFW" /></case>
					<case value="GK"><link block="LGK" /></case>
					<case value="MG"><link block="LMG" /></case>
					<case value="COACH"><link block="LCOACH" /></case>
					<case value="COMBI"><link block="LCOMBI" /></case>				
				</switch>
			</conditional>
			<conditional>
				<switch property="Rarity">
					<case value="2"><link block="L2" /></case>
					<case value="3"><link block="L3" /></case>
					<case value="4"><link block="L4" /></case>
					<case value="5"><link block="L5" /></case>
					<case value="6"><link block="L6" /></case>
				</switch>
			</conditional>
		</overlayblocks>
		<textblocks>
			<conditional>
				<if property="Rarity" contains="6"><link block="licenseName+" ><property name="Name" /></link></if>
				<elseif property="SubType" contains="Hissatsu"><link block="licenseName+"><property name="Name" /></link></elseif>
			</conditional>
			<link block="licenseName"><property name="Name"/></link>
			<link block="licenseTeam"><property name="Team"/></link>
			<link block="licenseNumber"><property name="Number"/></link>
		</textblocks>
	</template>

